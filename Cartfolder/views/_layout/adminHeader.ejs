          </li>
          
        </ul>
        
      </div>
    </nav>

    <main role="main" class="container">
      <div class="starter-template">
      </div>
      <%- messages('messages', locals) %>
      <% if (errors) {%>
        <% errors.forEach(function(error) { %>
            <div class="alert alert-danger">
                <%= error.msg %>
            </div>
            <% }); %>
        <% } %>

    
