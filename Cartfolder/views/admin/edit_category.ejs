<%- include ("../_layout/adminHeader.ejs") %> 
<h2 class="page-title">Edit a Category</h2>
<a href="/admin/pages" class="btn btn-primary">Back to all Categories</a>
<br><br>
<form method="post" action="/admin/categories/edit-category/<%= id %>">
    <div class="form-group">
        <label for="">Title</label>
        <input type="text" class="form-control" name="title" value="<%= title %>" placeholder="Title">
    </div>
    <button class="btn btn-default">Submit</button>
</form>

<%- include ("../_layout/adminFooter.ejs") %>